{
  "name": "gulp-posthtml",
  "version": "3.0.1",
  "description": "Gulp PostHTML Plugin",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "standard",
    "test": "jest --verbose --coverage",
    "docs": "jsdoc2md index.js > INDEX.md",
    "clean": "rm -rf jest coverage jsdoc-api dmd",
    "start": "npm run clean && npm run lint && npm test",
    "release": "standard-version"
  },
  "dependencies": {
    "gulp-util": "^3.0.6",
    "posthtml": "^0.10.1",
    "posthtml-load-config": "^1.0.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.16",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "posthtml-include": "^1.1.0",
    "posthtml-sugarml": "^1.0.0-alpha3",
    "standard": "^9.0.1",
    "standard-version": "^4.0.0",
    "vinyl": "^2.0.1"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "gulp",
    "gulpplugin",
    "gulp-posthtml"
  ],
  "author": {
    "name": "Ivan Voishev",
    "email": "voischev.ivan@ya.ru"
  },
  "contributors": [
    {
      "name": "Michael Ciniawsky",
      "email": "michael.ciniawsky@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posthtml/gulp-posthtml.git"
  },
  "bugs": {
    "url": "https://github.com/posthtml/gulp-posthtml/issues"
  },
  "homepage": "https://github.com/posthtml/gulp-posthtml#readme",
  "license": "MIT"
}
